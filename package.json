{
  "name": "majinbuu",
  "version": "0.4.3",
  "description": "coming soon",
  "main": "index.mjs",
  "scripts": {
    "levenstein": "gcc levenstein.c -O3 -o levenstein",
    "build": "npm run transpile && npm run minify && npm run minify-es5 && npm test && npm run size && npm run size-es5",
    "transpile": "babel ./index.mjs --out-file ./index.js --presets=es2015",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "minify": "uglifyjs index.mjs --comments=/^!/ --compress --mangle -o min.mjs",
    "minify-es5": "uglifyjs index.js --comments=/^!/ --compress --mangle -o min.js",
    "size": "cat index.mjs | wc -c;cat min.mjs | wc -c;gzip -c min.mjs | wc -c",
    "size-es5": "cat index.js | wc -c;cat min.js | wc -c;gzip -c min.js | wc -c",
    "test": "istanbul cover test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebReflection/majinbuu.git"
  },
  "keywords": [
    "morph",
    "mutate",
    "transform",
    "array",
    "levenshtein"
  ],
  "author": "Andrea Giammarchi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WebReflection/majinbuu/issues"
  },
  "homepage": "https://github.com/WebReflection/majinbuu#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^2.13.1",
    "istanbul": "^0.4.5",
    "tressa": "^0.3.1",
    "uglify-es": "^3.1.3"
  }
}
